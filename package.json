{
  "name": "express-api",
  "version": "0.0.1",
  "description": "express-api",
  "scripts": {
    "dev": "npx env-cmd -f .env.development ts-node-dev -r tsconfig-paths/register ./src/server.ts",
    "start": "node ./dist/server.js",
    "test": "npx env-cmd -f .env.testing -- jest --config jest.config.ts --projects ./test --forceExit --runInBand --detectOpenHandles --coverage",
    "test:dev": "npx env-cmd -f .env.development -- jest --config jest.config.ts --projects ./test --forceExit --runInBand --detectOpenHandles --watch --coverage",
    "build": "tsc --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorwanbarros/node-express-api.git"
  },
  "author": "igorwanbarros",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/igorwanbarros/node-express-api/issues"
  },
  "homepage": "https://github.com/igorwanbarros/node-express-api#readme",
  "dependencies": {
    "@decorators/express": "^3.0.0",
    "@prisma/client": "^5.3.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-openapi": "^12.1.3",
    "joi": "^17.11.0",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.3.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@jest-mock/express": "^2.0.2",
    "@types/body-parser": "^1.19.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.9",
    "@types/supertest": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "_moduleAliases": {
    "@domain": "./src/domain",
    "@infra": "./src/infra",
    "@utils": "./src/utils",
    "@docs": "./src/api-docs"
  }
}
